{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Forwarding": {
    "BaseUrl": "{your-n8n-url}",
    "HealthUrl": "{your-n8n-url}/healthz",
    "TimeoutSeconds": 10,
    "PathPrefix": "webhook"
  },
  "Queue": {
    "ConnectionString": "{storageAccount}",
    "QueueName": "webhooks",
    "VisibilityTimeoutSeconds": 30
  },
  "Validation": {
    "BasePath": "validations",
    "Mode": "permissive"
  },
  "Cors": {
    "AllowedOrigins": [ "*" ]
  },
  "Worker": {
    "PollIntervalSeconds": 30,
    "BatchSize": 16,
    "HealthCheckIntervalSeconds": 60,
    "HealthFailureThreshold": 3,
    "HealthSuccessThreshold": 3
  }
}
